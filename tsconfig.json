{
    "compilerOptions": {
        "target": "ES5",//编译为es5
        "module": "ES2015",
        "strict": true,
        "esModuleInterop": true,//允许使用es6 导入导出
        "skipLibCheck": true,//跳过lib检查
        "forceConsistentCasingInFileNames": false,//是否强制检查文件名称
        "jsx": "react",//types 使用jsx
        "moduleResolution": "node",
        "baseUrl": "./",
        "paths": {
            "@/*":["src/*"]
        }
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
}